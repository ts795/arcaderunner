  "bugs": {
    "url": "https://github.com/ts795/arcaderunner/issues"
  },
  "homepage": "https://github.com/ts795/arcaderunner#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.0",
    "concurrently": "^6.2.1",
    "connect-session-sequelize": "^7.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.0",
    "nodemon": "^2.0.12",
    "sequelize": "^6.6.5"
  }
}
